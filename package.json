{
  "name": "ngx-block-loading-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "serve": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version:patch": "cd projects/ngx-block-loading && npm version patch",
    "version:minor": "cd projects/ngx-block-loading && npm version minor",
    "version:major": "cd projects/ngx-block-loading && npm version major",
    "build:lib": "ng build --project=ngx-block-loading",
    "build:lib:prod": "ng build --project=ngx-block-loading --configuration production",
    "watch:lib": "ng build --project=ngx-block-loading --watch",
    "watch:lib:prod": "ng build --project=ngx-block-loading --watch --configuration production",
    "publish": "cd dist/ngx-block-loading && npm publish",
    "start:api": "json-server json-server/db.json --route json-server/routes.json --no-cors true --delay 5000"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "json-server": "^0.16.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.16",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
